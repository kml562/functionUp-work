{
  "name": "kaop",
  "version": "2.7.10",
  "description": "simple js OOP/AOP lib helper with classes, inheritance and method Advices (method interceptors)",
  "main": "root.js",
  "jest": {
    "verbose": true
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "version": "browserify ./lib/index.js -o ./dist/kaop-lib.dev.js ; uglifyjs ./dist/kaop-lib.dev.js --compress --mangle --keep-fnames --output ./dist/kaop-lib.min.js",
    "preversion": "jest --coverage && cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js ; git commit --amend",
    "postversion": "git push origin master --follow-tags",
    "report-cover": "./coverage.s",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "framework",
    "helper",
    "oop",
    "aop"
  ],
  "author": "k1r0s",
  "repository": {
    "type": "git",
    "url": "https://github.com/k1r0s/kaop.git"
  },
  "homepage": "https://github.com/k1r0s/kaop",
  "bugs": {
    "url": "https://github.com/k1r0s/kaop/issues"
  },
  "license": "ISC",
  "devDependencies": {
    "browserify": "^14.1.0",
    "coveralls": "^2.12.0",
    "jest": "^19.0.2",
    "uglifyjs": "^2.4.10",
    "semantic-release": "^6.3.2"
  }
}
